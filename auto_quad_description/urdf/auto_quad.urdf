<?xml version="1.0"?>
<robot name="auto_quad">
    <!-- Base Link -->
    <link name="base_link">
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.2 0.2 0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.2 0.2 0.1"/>
            </geometry>
        </collision>
    </link>

    <!-- Front Left Motor -->
    <link name="front_left_motor">
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.02"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.02"/>
            </geometry>
        </collision>
    </link>

    <joint name="front_left_motor_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_left_motor"/>
        <origin xyz="0.1 0.1 0" rpy="0 0 0"/>
    </joint>

    <!-- Front Right Motor -->
    <link name="front_right_motor">
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.02"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.02"/>
            </geometry>
        </collision>
    </link>

    <joint name="front_right_motor_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_right_motor"/>
        <origin xyz="-0.1 0.1 0" rpy="0 0 0"/>
    </joint>
    
     <!-- Back Left Motor -->
    <link name="back_left_motor">
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.02"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.02"/>
            </geometry>
        </collision>
    </link>

    <joint name="back_left_motor_joint" type="fixed">
        <parent link="base_link"/>
        <child link="back_left_motor"/>
        <origin xyz="0.1 -0.1 0" rpy="0 0 0"/>
    </joint>
    
    <!-- Front Left Motor -->
    <link name="back_right_motor">
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.02"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.02"/>
            </geometry>
        </collision>
    </link>

    <joint name="back_right_motor_joint" type="fixed">
        <parent link="base_link"/>
        <child link="back_right_motor"/>
        <origin xyz="-0.1 -0.1 0" rpy="0 0 0"/>
    </joint>




</robot>
